  "liveNow": false,
  "panels": [
    {
      "id": 1,
      "title": "Quality Score %",
      "description": "Percentage of data quality checks passing (pass / total * 100)",
      "type": "gauge",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 0 },
      "datasource": { "type": "grafana-bigquery-datasource", "uid": "bigquery-mrhealth" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": 80 },
              { "color": "yellow", "value": 90 },
              { "color": "green", "value": 95 }
            ]
          },
          "min": 0,
          "max": 100,
          "unit": "percent"
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "targets": [
        {
          "rawSql": "SELECT ROUND(SAFE_DIVIDE(COUNTIF(status = 'PASS'), COUNT(*)) * 100, 2) AS quality_score FROM `sixth-foundry-485810-e5.case_ficticio_monitoring.pipeline_metrics` WHERE metric_name LIKE 'dq_check_%' AND DATE(recorded_at) = CURRENT_DATE()",
          "format": "table",
          "refId": "A"
        }
      ]
    },
    {
      "id": 2,
      "title": "Check Results (Last Execution)",
      "description": "Individual data quality check results from the most recent run",
      "type": "table",
      "gridPos": { "h": 10, "w": 16, "x": 8, "y": 0 },
      "datasource": { "type": "grafana-bigquery-datasource", "uid": "bigquery-mrhealth" },
      "fieldConfig": {
        "defaults": {},
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "status" },
            "properties": [
              {
                "id": "custom.displayMode",
                "value": "color-background-solid"
              },
              {
                "id": "mappings",
                "value": [
                  { "options": { "PASS": { "color": "green", "index": 0 } }, "type": "value" },
                  { "options": { "FAIL": { "color": "red", "index": 1 } }, "type": "value" },
                  { "options": { "WARN": { "color": "yellow", "index": 2 } }, "type": "value" }
                ]
              }
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "sortBy": [{ "desc": false, "displayName": "status" }]
      },
      "targets": [
        {
          "rawSql": "SELECT metric_name AS check_name, status, CAST(metric_value AS STRING) AS details, recorded_at FROM `sixth-foundry-485810-e5.case_ficticio_monitoring.pipeline_metrics` WHERE metric_name LIKE 'dq_check_%' AND recorded_at = ( SELECT MAX(recorded_at) FROM `sixth-foundry-485810-e5.case_ficticio_monitoring.pipeline_metrics` WHERE metric_name LIKE 'dq_check_%' ) ORDER BY status DESC, metric_name",
          "format": "table",
          "refId": "A"
        }
      ]
    },
    {
      "id": 3,
      "title": "Quality Score History (30 days)",
      "description": "Daily quality score trend over the last 30 days",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 16, "x": 0, "y": 10 },
      "datasource": { "type": "grafana-bigquery-datasource", "uid": "bigquery-mrhealth" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisCenteredZero": false,
            "axisLabel": "Quality %",
            "drawStyle": "line",
            "fillOpacity": 20,
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "always",
            "spanNulls": false,
            "thresholdsStyle": { "mode": "line" }
          },
          "min": 0,
          "max": 100,
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 95 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "calcs": ["mean", "min"], "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "single", "sort": "none" }
      },
      "targets": [
        {
          "rawSql": "SELECT TIMESTAMP(run_date) AS time, quality_score FROM ( SELECT DATE(recorded_at) AS run_date, ROUND(SAFE_DIVIDE(COUNTIF(status = 'PASS'), COUNT(*)) * 100, 2) AS quality_score FROM `sixth-foundry-485810-e5.case_ficticio_monitoring.pipeline_metrics` WHERE metric_name LIKE 'dq_check_%' AND recorded_at >= TIMESTAMP_SUB(CURRENT_TIMESTAMP(), INTERVAL 30 DAY) GROUP BY run_date ) ORDER BY run_date",
          "format": "time_series",
          "refId": "A"
        }
      ]
    },
    {
      "id": 4,
      "title": "Failed Checks Count",
      "description": "Number of data quality checks currently failing",
      "type": "stat",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 10 },
      "datasource": { "type": "grafana-bigquery-datasource", "uid": "bigquery-mrhealth" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 3 }
            ]
          },
          "unit": "short"
        },
        "overrides": []
      },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "textMode": "value_and_name",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        }
      },
      "targets": [
        {
          "rawSql": "SELECT COUNTIF(status = 'FAIL') AS failed_checks FROM `sixth-foundry-485810-e5.case_ficticio_monitoring.pipeline_metrics` WHERE metric_name LIKE 'dq_check_%' AND recorded_at = ( SELECT MAX(recorded_at) FROM `sixth-foundry-485810-e5.case_ficticio_monitoring.pipeline_metrics` WHERE metric_name LIKE 'dq_check_%' )",
          "format": "table",
          "refId": "A"
        }
      ]
    }
  ],
