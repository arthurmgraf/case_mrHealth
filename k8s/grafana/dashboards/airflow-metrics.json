  "liveNow": false,
  "panels": [
    {
      "id": 1,
      "title": "Task Duration by DAG (7 days)",
      "description": "Heatmap of task execution durations grouped by DAG over the last 7 days",
      "type": "heatmap",
      "gridPos": { "h": 10, "w": 14, "x": 0, "y": 0 },
      "datasource": { "type": "grafana-bigquery-datasource", "uid": "bigquery-mrhealth" },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "scheme",
            "schemeId": "Oranges"
          },
          "custom": {
            "hideFrom": { "legend": false, "tooltip": false, "viz": false }
          }
        },
        "overrides": []
      },
      "options": {
        "calculate": true,
        "calculation": {
          "xBuckets": { "mode": "size", "value": "1d" },
          "yBuckets": { "mode": "size" }
        },
        "cellGap": 1,
        "color": {
          "exponent": 0.5,
          "fill": "dark-orange",
          "mode": "scheme",
          "scheme": "Oranges",
          "steps": 64
        },
        "exemplars": { "color": "rgba(255,0,255,0.7)" },
        "filterValues": { "le": 1e-09 },
        "tooltip": { "show": true, "yHistogram": false },
        "yAxis": { "axisLabel": "Duration (s)", "unit": "s" }
      },
      "targets": [
        {
          "rawSql": "SELECT recorded_at AS time, CAST(metric_value AS FLOAT64) AS duration_seconds FROM `sixth-foundry-485810-e5.case_ficticio_monitoring.pipeline_metrics` WHERE metric_name LIKE 'task_duration_%' AND recorded_at >= TIMESTAMP_SUB(CURRENT_TIMESTAMP(), INTERVAL 7 DAY) ORDER BY recorded_at",
          "format": "time_series",
          "refId": "A"
        }
      ]
    },
    {
      "id": 2,
      "title": "DAGs Run Today",
      "description": "Total number of DAG runs executed today",
      "type": "stat",
      "gridPos": { "h": 5, "w": 5, "x": 14, "y": 0 },
      "datasource": { "type": "grafana-bigquery-datasource", "uid": "bigquery-mrhealth" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null }
            ]
          },
          "unit": "short"
        },
        "overrides": []
      },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "textMode": "value_and_name",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        }
      },
      "targets": [
        {
          "rawSql": "SELECT COUNT(*) AS dags_run_today FROM `sixth-foundry-485810-e5.case_ficticio_monitoring.pipeline_metrics` WHERE metric_name = 'pipeline_run' AND DATE(recorded_at) = CURRENT_DATE()",
          "format": "table",
          "refId": "A"
        }
      ]
    },
    {
      "id": 3,
      "title": "Top 5 Slowest Tasks",
      "description": "Slowest tasks by average duration over the last 7 days",
      "type": "table",
      "gridPos": { "h": 10, "w": 10, "x": 14, "y": 5 },
      "datasource": { "type": "grafana-bigquery-datasource", "uid": "bigquery-mrhealth" },
      "fieldConfig": {
        "defaults": {
          "custom": { "align": "auto", "inspect": false }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "avg_duration_s" },
            "properties": [
              { "id": "unit", "value": "s" },
              {
                "id": "custom.displayMode",
                "value": "gradient-gauge"
              },
              {
                "id": "color",
                "value": { "mode": "continuous-YlOrRd" }
              }
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "sortBy": [{ "desc": true, "displayName": "avg_duration_s" }]
      },
      "targets": [
        {
          "rawSql": "SELECT REPLACE(metric_name, 'task_duration_', '') AS task_name, ROUND(AVG(CAST(metric_value AS FLOAT64)), 2) AS avg_duration_s, ROUND(MAX(CAST(metric_value AS FLOAT64)), 2) AS max_duration_s, COUNT(*) AS executions FROM `sixth-foundry-485810-e5.case_ficticio_monitoring.pipeline_metrics` WHERE metric_name LIKE 'task_duration_%' AND recorded_at >= TIMESTAMP_SUB(CURRENT_TIMESTAMP(), INTERVAL 7 DAY) GROUP BY metric_name ORDER BY avg_duration_s DESC LIMIT 5",
          "format": "table",
          "refId": "A"
        }
      ]
    },
    {
      "id": 4,
      "title": "SLA Misses (30 days)",
      "description": "SLA miss events over the last 30 days",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 14, "x": 0, "y": 10 },
      "datasource": { "type": "grafana-bigquery-datasource", "uid": "bigquery-mrhealth" },
      "fieldConfig": {
        "defaults": {
          "color": { "fixedColor": "red", "mode": "fixed" },
          "custom": {
            "axisCenteredZero": false,
            "axisLabel": "SLA Misses",
            "drawStyle": "bars",
            "fillOpacity": 80,
            "lineWidth": 1,
            "pointSize": 5,
            "showPoints": "never",
            "spanNulls": false
          },
          "unit": "short"
        },
        "overrides": []
      },
      "options": {
        "legend": { "calcs": ["sum"], "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single", "sort": "none" }
      },
      "targets": [
        {
          "rawSql": "SELECT TIMESTAMP(miss_date) AS time, sla_misses FROM ( SELECT DATE(recorded_at) AS miss_date, COUNT(*) AS sla_misses FROM `sixth-foundry-485810-e5.case_ficticio_monitoring.pipeline_metrics` WHERE metric_name = 'sla_miss' AND recorded_at >= TIMESTAMP_SUB(CURRENT_TIMESTAMP(), INTERVAL 30 DAY) GROUP BY miss_date ) ORDER BY miss_date",
          "format": "time_series",
          "refId": "A"
        }
      ]
    }
  ],
