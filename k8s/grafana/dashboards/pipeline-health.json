    {
      "id": 1,
      "title": "Pipeline Status",
      "description": "Last pipeline run status (SUCCESS / FAILED)",
      "type": "stat",
      "gridPos": { "h": 6, "w": 6, "x": 0, "y": 0 },
      "datasource": { "type": "grafana-bigquery-datasource", "uid": "bigquery-mrhealth" },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "options": { "SUCCESS": { "color": "green", "index": 0, "text": "SUCCESS" } }, "type": "value" },
            { "options": { "FAILED": { "color": "red", "index": 1, "text": "FAILED" } }, "type": "value" }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "textMode": "value",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        }
      },
      "targets": [
        {
          "rawSql": "SELECT status FROM `sixth-foundry-485810-e5.case_ficticio_monitoring.pipeline_metrics` WHERE metric_name = 'pipeline_run' ORDER BY recorded_at DESC LIMIT 1",
          "refId": "A"
        }
      ]
    },
    {
      "id": 2,
      "title": "Pipeline Duration (7 days)",
      "description": "Pipeline execution duration over the last 7 days",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 18, "x": 6, "y": 0 },
      "datasource": { "type": "grafana-bigquery-datasource", "uid": "bigquery-mrhealth" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisCenteredZero": false,
            "axisLabel": "Duration (seconds)",
            "drawStyle": "line",
            "fillOpacity": 15,
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "auto",
            "spanNulls": false
          },
          "unit": "s"
        },
        "overrides": []
      },
      "options": {
        "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "single", "sort": "none" }
      },
      "targets": [
        {
          "rawSql": "SELECT recorded_at AS time, CAST(metric_value AS FLOAT64) AS duration_seconds FROM `sixth-foundry-485810-e5.case_ficticio_monitoring.pipeline_metrics` WHERE metric_name = 'pipeline_duration_seconds' AND recorded_at >= TIMESTAMP_SUB(CURRENT_TIMESTAMP(), INTERVAL 7 DAY) ORDER BY recorded_at",
          "refId": "A"
        }
      ]
    },
    {
      "id": 3,
      "title": "Failed Tasks (24h)",
      "description": "Tasks that failed in the last 24 hours",
      "type": "table",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "datasource": { "type": "grafana-bigquery-datasource", "uid": "bigquery-mrhealth" },
      "fieldConfig": {
        "defaults": {},
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "status" },
            "properties": [
              { "id": "custom.displayMode", "value": "color-text" },
              { "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "sortBy": [{ "desc": true, "displayName": "recorded_at" }]
      },
      "targets": [
        {
          "rawSql": "SELECT recorded_at, metric_name AS task_name, status, CAST(metric_value AS STRING) AS details FROM `sixth-foundry-485810-e5.case_ficticio_monitoring.pipeline_metrics` WHERE status = 'FAILED' AND recorded_at >= TIMESTAMP_SUB(CURRENT_TIMESTAMP(), INTERVAL 24 HOUR) ORDER BY recorded_at DESC LIMIT 50",
          "refId": "A"
        }
      ]
    },
    {
      "id": 4,
      "title": "Rows Processed Today",
      "description": "Total rows processed in today's pipeline run",
      "type": "stat",
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 8 },
      "datasource": { "type": "grafana-bigquery-datasource", "uid": "bigquery-mrhealth" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 100 },
              { "color": "green", "value": 1000 }
            ]
          },
          "unit": "short"
        },
        "overrides": []
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "center",
        "textMode": "value_and_name",
        "reduceOptions": {
          "calcs": ["sum"],
          "fields": "",
          "values": false
        }
      },
      "targets": [
        {
          "rawSql": "SELECT CAST(metric_value AS FLOAT64) AS rows_processed FROM `sixth-foundry-485810-e5.case_ficticio_monitoring.pipeline_metrics` WHERE metric_name = 'rows_processed' AND DATE(recorded_at) = CURRENT_DATE() ORDER BY recorded_at DESC LIMIT 1",
          "refId": "A"
        }
      ]
    }
  ],
